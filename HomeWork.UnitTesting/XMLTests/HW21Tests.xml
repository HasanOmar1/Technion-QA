		<!-- Input -->
		<string>abddba</string>

		<!-- Output-->
		<expectedResult>true</expectedResult>
	</IsSpecialString>

	<IsSpecialString>
		<!-- Input -->
		<string>ZZRRYYYY</string>

		<!-- Output-->
